{
  "name": "13k-bot",
  "description": "13kjs automatic submission validation",
  "version": "0.1.0",
  "engines": {
    "node": "10.16.0"
  },
  "dependencies": {
    "aws-sdk": "2.495.0",
    "base-64": "0.1.0",
    "get-pixels": "3.3.2",
    "github-app": "4.0.1",
    "mime-types": "2.1.24",
    "node-fetch": "2.6.0",
    "node-zip": "1.1.1",
    "selenium-webdriver": "4.0.0-alpha.4"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/base-64": "0.1.3",
    "@types/github": "7.1.0",
    "@types/jest": "24.0.15",
    "@types/node-fetch": "2.3.7",
    "@types/selenium-webdriver": "4.0.1",
    "babel-jest": "24.8.0",
    "babel-plugin-inline-dotenv": "1.3.3",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "serverless": "1.48.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "deploy": "yarn build; serverless deploy",
    "build": "babel src --extensions \".ts\" --out-dir build --ignore **/__tests__ --ignore **/__fixtures__",
    "test": "jest --config ./jest.config.js --no-cache",
    "type": "tsc --noEmit",
    "lint": "yarn prettier -c './**/*.[tj]s'",
    "dev": "babel-node --extensions .ts ./src/dev-server.ts",
    "test0": "babel-node --extensions .ts ./src/test.ts"
  }
}
